##CPU statistics
UserParameter=proc.count.queue, vmstat | awk {'print $1'} | tail -1
UserParameter=proc.io.queue, vmstat | awk {'print $2'} | tail -1
#Timezone
UserParameter=host.os.timezone,timedatectl | grep 'Time zone' | awk {'print $3'}
#HDD statistic
UserParameter=iostat.collect,/etc/zabbix/scripts/iostat-collect.sh /tmp/iostat.out 8 || echo 1
UserParameter=iostat.metric[*],/etc/zabbix/scripts/iostat-parse.sh /tmp/iostat.out $1 $2

UserParameter=iostat.discovery, iostat -d | awk 'BEGIN {check=0;count=0;array[0]=0;} {if(check==1 && $1 != ""){array[count]=$1;count=count+1;}if($1=="Device:"){check=1;}} END {printf("{\n\t\"data\":[\n");for(i=0;i<count;++i){printf("\t\t{\n\t\t\t\"{#HARDDISK}\":\"%s\"}", array[i]); if(i+1<count){printf(",\n");}} printf("]}\n");}'

UserParameter=iptables.status, lsmod | grep ip_tables | wc -l

UserParameter=hdd.discovery, /etc/zabbix/scripts/zabbix_hdd.py --discovery true
UserParameter=hdd.selftest[*], echo $(smartctl -d sat --all $1|grep "SMART overall-health self-assessment test result"|awk {'print $6'})

